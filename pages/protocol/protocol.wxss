page {
  padding: 30rpx;
  box-sizing: border-box;
  background: #fff;
}

.padding-box {
  height: 100rpx;
}

.title, .content-title {
  font-size: 38rpx;
  color: #000;
  margin: 20rpx 0;
  font-weight: bold;
}

.content-title {
  font-size: 32rpx;
  text-align: left;
}

.content {
  text-indent: 60rpx;
  color: #333;
  line-height: 46rpx;
  font-size: 32rpx;
}

.protocol-hint {
  font-size: 28rpx;
  color: #0f73cb;
  position: fixed;
  padding: 30rpx 30rpx 30rpx 80rpx;
  width: 100%;
  left: 0;
  bottom: 0;
  box-sizing: border-box;
}

.btn {
  display: flex;
  position: fixed;
  width: 100%;
  padding: 30rpx;
  left: 0;
  bottom: 0;
  background: #fff;
  box-sizing: border-box;
}

.btn view, .btn navigator {
  margin: 0 auto;
  font-size: 30rpx;
  background: #0f73cb;
  color: #fff;
  line-height: 54rpx;
  border-radius: 50rpx;
  padding: 0 80rpx;
}

.btn navigator {
  background: #ccc;
  color: #333;
}
